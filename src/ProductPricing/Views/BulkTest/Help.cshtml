@{
    ViewData["Title"] = "Bult Tester Help";
}

<div>
    <h4>Bulk Tester Help</h4>
    <hr />
    <p>Please note: File format is CSV (Comma Delimited). Fields should be in the exact order below.</p>

    <table class="table table-striped table-condensed table-bordered">
        <thead class="thead-inverse">
            <tr><th>Field No</th><th>Field Name</th><th>Description</th></tr>
        </thead>
        <tbody>
            <tr><td>1</td><td>Sr #</td><td>Test Number</td></tr>
            <tr><td>2</td><td>Variant</td><td>Product Name</td></tr>
            <tr><td>3</td><td>Plan</td><td>Plan Name</td></tr>
            <tr><td>4</td><td> Sum Insured </td><td>Sum Insured. Numberic string</td></tr>
            <tr><td>5</td><td>Co- Morbid Conditions</td><td>Co-Morbitity Conditions seperated by "&" </td></tr>
            <tr><td>6</td><td>Ind/FF</td><td>Individual or "FF" for Family floater</td></tr>
            <tr><td>7</td><td>Gender</td><td>M" or "F"</td></tr>
            <tr><td>8</td><td>Age*</td><td>Age in years </td></tr>
            <tr><td>9</td><td>Family Structure</td><td>Family Structure number. Not Used. Field 20 being used</td></tr>
            <tr><td>10</td><td>Zone</td><td>"I" or "II" or "III" </td></tr>
            <tr><td>11</td><td>Room Type</td><td>"Single", "Shared","General","Any"</td></tr>
            <tr><td>12</td><td>Channel</td><td>Direct or Staff</td></tr>
            <tr><td>13</td><td>Policy Term</td><td>1 or 2 or 3 years</td></tr>
            <tr><td>14</td><td>Deductible</td><td>Numeric string, else Zero</td></tr>
            <tr><td>15</td><td>OPD Expense</td><td>Numeric string, else Zero</td></tr>
            <tr><td>16</td><td>Hospital Cash</td><td>Numeric string, else Zero</td></tr>
            <tr><td>17</td><td>Maternity Benefit</td><td>"Yes" or Ignored </td></tr>
            <tr><td>18</td><td>Waiver of mandatory Co-pay</td><td>"Yes" or Ignored </td></tr>
            <tr><td>19</td><td>Premium excluding Stamp duty and Service Tax</td><td>Input premium used to compare against calculated premium</td></tr>
            <tr><td>20</td><td>Family definition</td><td>Family Structure seperated by "+" sign.</td></tr>
            </tbody>
        </table>
</div>
    <div>
        <a asp-action="Index">Back to Bulk Tester</a>
    </div>
